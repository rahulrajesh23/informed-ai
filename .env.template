# General App Config
APP_ENV=DEV

# PostgreSQL Config
DB_USERNAME=postgres
DB_PASSWORD=your_password
DB_SERVER_NAME=localhost  # use host.docker.internal if building via docker
DB_PORT=5432
DB_DATABASE_NAME=your_db_name
DB_URL=postgresql://postgres:your_password@localhost:5432/your_db_name
# Syntax is postgresql://<username>:<password>@<db_server>:<postgres_port>/<db_name>

# OpenAI GPT Config
GPT_APIKEY=openai_gpt_key
GPT_MODEL_NAME=gpt-3.5-turbo
